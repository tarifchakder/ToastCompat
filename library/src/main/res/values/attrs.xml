<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ToastCompat">
        <attr name="toastTextBold" format="boolean" />
        <attr name="toastTextColor" format="color" />
        <attr name="toastFont" format="reference" />
        <attr name="toastDrawableBackground" format="reference" />
        <attr name="toastDrawableTint" format="color" />
        <attr name="toastTextSize" format="dimension" />
        <attr name="toastColorBackground" format="color" />
        <attr name="toastBackgroundType" format="enum">
            <enum name="GRADIENT" value="2" />
            <enum name="SOLID" value="1" />
        </attr>
        <attr name="toastGradientStartColor" format="color" />
        <attr name="toastGradientEndColor" format="color" />
        <attr name="toastRadius" format="dimension" />
        <attr name="toastStrokeWidth" format="dimension" />
        <attr name="toastStrokeColor" format="color" />
        <attr name="toastIconStart" format="reference" />
        <attr name="toastIconEnd" format="reference" />
        <attr name="toastIconSize" format="integer" />
        <attr name="toastLength" format="integer">
            <enum name="LONG" value="1" />
            <enum name="SHORT" value="0" />
        </attr>
        <attr name="toastGravity" format="integer">
            <enum name="CENTER" value="1" />
            <enum name="TOP" value="2" />
            <enum name="BOTTOM" value="3" />
            <enum name="LEFT" value="3" />
        </attr>

    </declare-styleable>
</resources>